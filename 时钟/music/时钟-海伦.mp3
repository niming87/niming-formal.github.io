    <script type="text/javascript">
    $(function () {
        window.mp3_id = '470503';
        window.play_id = 'RV5eUnBVGV9QQltXUXZeVl8=';
        window.mp3_url = '';
        window.mp3_title = '时钟';
        window.mp3_author = '海伦';
        window.mp3_name = mp3_title + '-' + mp3_author;
        window.mp3_type = 0;
        window.mp3_cover = 'http://img4.kuwo.cn/star/albumcover/500/s3s15/61/712850580.jpg';
        window.error_msg = '';
        window.is_white_url = false;
        window.mp3_extra_url = 'https://pan.xunlei.com/s/VO8edzTK0b959BU5KRCMfpn1A1?pwd=bdnq#';
        window.ap = new APlayer({
            container: document.getElementById('aplayer'),
            autoplay: false,
            theme: '#0e90d2',
            lrcType: 1,
            preload: 'metadata',
            // loop: 'none',
            order: 'list'
        });

        window.btn_download_mp3 = $('#btn-download-mp3');
        window.btn_download_lrc = $('#btn-download-lrc');

        ap.on('listswitch', function () {
            layer.tips('点击这里播放', '.aplayer-button', {
                tips: [3, '#FF5722'],
            });
        });
        
                
        ap.on('error', function (error) {
            if (error_msg.length === 0) {
                if (is_white_url || mp3_extra_url.length > 0) {
                    error_msg = '当前歌曲在线播放出错，您可以下载到本地播放~';
                } else {
                    error_msg = '当前歌曲解析出错，暂无法下载';
                }
            }
            ap.notice('<span class="font-weight-bolder" style="color:#FF5722;background-color:#f4f4f5"><i class="fa fa-exclamation-circle" aria-hidden="true"></i> ' + error_msg + '</span>', 3000, 1);
        });
        $('.aplayer-pic').click(function () {
            if (error_msg) {
                ap.notice('<span class="font-weight-bolder" style="color:#FF5722;background-color:#f4f4f5"><i class="fa fa-exclamation-circle" aria-hidden="true"></i> ' + error_msg + '</span>', 3000, 1);
            }
        });
        
        
                
        btn_download_mp3.click(function () {
            
            
            

            //是否有错误信息
            if (is_white_url === false && error_msg.length > 0 && mp3_extra_url.length === 0) {
                jAlert(error_msg);
                return false;
            }

            $.ajaxSettings.async = false;
            let adShow = false;

            $.ajax({
                type: "POST",
                url: "/api/ad-handle",
                dataType: "json",
                data: {
                    "mid": mp3_id,
                    "m_type": 1
                },
                success: function (data) {
                    if (data.code === 1) {
                        return true;
                    } else if (data.code === 2) {
                        $('#ad-modal-prompt').html('');
                        $('#ad-modal-prepend').html('');
                        $('#ad-modal-body').html(data.data.content);
                        $('#ad-modal').modal('toggle');
                        adShow = true;
                        return false;
                    } else if (data.code === 3) {
                        $('#ad-modal-prompt').html(data.data.answerPrompt);
                        $('#ad-modal-prepend').html(data.data.answerFormHtml);
                        $('#ad-modal-body').html(data.data.content);
                        $('#ad-modal').modal('toggle');
                        adShow = true;
                        return false;
                    } else if (data.code === 0) {
                        jAlert(data.msg);
                        adShow = true;
                        return false;
                    }
                },
                error: function (e) {
                    jAlert('请求失败，请重试！');
                    adShow = true;
                    return false;
                },
            });

            $.ajaxSettings.async = true;
            if (adShow) return false;

                                
            if (is_qq_wx_tb()) {
                return false;
            }

            if (mp3_extra_url.length !== 0) {
                                $.alert({
                    title: '<span style="color: #16b777;">请选择音质</span>',
                    content: `
<div class="d-flex justify-content-between">
<a href="${mp3_url}" download="${mp3_name}.mp3" class="btn btn-link" target="_blank" onclick="downloadLog(mp3_id)">下载低品质MP3</a>
<a href="${mp3_extra_url}" class="btn btn-link" target="_blank" onclick="return extraUrlDo(mp3_id,mp3_extra_url)"><span class="badge badge-danger">推荐</span>下载高品质MP3</a>
</div>
`,
                    closeIcon: true,
                    typeAnimated: true,
                    backgroundDismiss: true,
                    buttons: false,
                    columnClass: 'col-md-5'
                });
                return false;
                            } else {
                downloadLog(mp3_id);
            }
        });

        //歌词按钮点击
        btn_download_lrc.click(function () {
            let textToDownload = $('#content-lrc').text();
            let blob = new Blob([textToDownload], {type: 'text/plain'});
            let url = URL.createObjectURL(blob);
            let a = document.createElement('a');
            a.style.display = 'none'; // 隐藏a元素
            a.href = url;
            a.download = window.mp3_name + '.lrc'; // 设置下载文件的名称
            document.body.appendChild(a);
            a.click();
            document.body.removeChild(a);
            window.URL.revokeObjectURL(url);
        });

        //获取歌曲播放地址
        $.ajax({
            type: "POST",
            url: "/api/play-url",
            dataType: "json",
            data: {id: play_id},
            success: function (result) {
                if (result.code === 1) {
                    mp3_url = result.data.url;
                    is_white_url = result.data.is_while_url;
                } else {
                    return jAlert(result.msg);
                }
            },
            error: function (e) {
                if (mp3_type === 0) {
                    if (e.status === 429) {
                        error_msg = '请求过于频繁，请稍后再试';
                        return jAlert(error_msg);
                    }
                    error_msg = '请求失败，请刷新重试！';
                    return jAlert(error_msg);
                }
            },
            complete: function () {
                addToList();
            }
        });


        
        $('.expand').click(function () {
            if ($(this).attr('data-status') === 'up') {
                $('.content-lrc').addClass('height-auto');
                $(this).attr('data-status', 'down')
                $(this).text('收起')
            } else {
                $('.content-lrc').removeClass('height-auto');
                $(this).attr('data-status', 'up')
                $(this).text('展开')
            }
        });

        
        setTimeout('autoScroll("#download-log")', 3500);
    });


    /** 方法区 **/

    async function addToList() {
        let lrc = $('#content-lrc').text();

        if (mp3_url.indexOf('antiserver.kuwo.cn') !== -1) {
            const url = new URL(mp3_url);
            url.searchParams.set('type', 'convert_url3');
            await $.getJSON(url.href, "callback=?", function (data) {
                if (data.code === 200) {
                    mp3_url = data.url
                }
            });
        }

        if (mp3_extra_url.length > 0) {
            btn_download_mp3.attr('href', mp3_extra_url);
        } else if (mp3_url.length > 0) {
            btn_download_mp3.attr('href', mp3_url);
            btn_download_mp3.attr('download', mp3_name + '.mp3');
        }

        ap.list.add([{
            name: mp3_title,
            artist: mp3_author,
            url: mp3_url,
            cover: mp3_cover,
            lrc: lrc
        }]);

        ap.template.dtime.innerHTML = '<span style="color: #999999;">04:09</span>';
    }

    
    function autoScroll(obj) {
        $(obj).find("ul").animate({
                marginTop: "-1rem"
            },
            1000,
            function () {
                $(this).css({
                    marginTop: "0"
                }).find("li:first").appendTo(this);
                setTimeout('autoScroll("#download-log")', 3500);
            });
    }

    
    function downloadLog(id, linkType = 1) {
        $.post("/api/download-log", {
            music_id: id,
            link_type: linkType
        });
    }


    
    function extraUrlDo(id, url) {
        downloadLog(id, 2);
                    return true;
    }

    function errFk(id) {
        jAlert(`
<ul class='list-group mt-3'>
    <li class='list-group-item'><span class='text-info'>当前歌曲：</span> <span
            style='color:#fa5661;font-weight: bolder'>${mp3_title}-${mp3_author}</span></li>
    <li class='list-group-item'>
        <span class='text-info'>错误类型：</span><br/>
        <div class='form-check form-check-inline'>
            <input name='musicErrType' class='form-check-input' type='checkbox' id='err-download' value='不能下载'>
            <label style='color:#ff5722' class='form-check-label' for='err-download'>不能下载 <small class="text-muted">
                (下载链接报错无法打开)
            </small></label>

        </div>
        <div class='form-check form-check-inline'>
            <input name='musicErrType' class='form-check-input' type='checkbox' id='err-match' value='歌曲不对'>
            <label style='color:#ff5722' class='form-check-label' for='err-match'>歌曲不对 <small class="text-muted">
                (播放/下载的歌曲与原歌名不符)
            </small></label>
        </div>
<div class='form-check form-check-inline'>
            <input name='musicErrType2' class='form-check-input' type='checkbox' id='err-other' value="1">
            <label style='color:#ff5722' class='form-check-label' for='err-other'>其他错误 <small style="color:#007bff">
                (手动输入错误描述信息)
            </small></label>
        </div>
<div class="form-group mt-1">
            <input class='form-control' style="display:none" placeholder="请输入错误描述（15个字符内）" type='text' id='err-other-input'>
  </div>
    </li>
</ul>
            `, 'blue', true, {
            onOpen: function () {
                $('input[name="musicErrType"]').change(function () {
                    if ($('input[name="musicErrType"]').is(':checked')) {
                        $("#err-other").prop("checked", false);
                        $('#err-other-input').hide();
                    }
                })

                $('#err-other').change(function () {
                    if ($(this).is(':checked')) {
                        $('input[name="musicErrType"]').prop("checked", false);
                        $('#err-other-input').show().focus();
                    } else {
                        $('#err-other-input').hide();
                    }
                })
            },
            columnClass: 'col-md-5',
            buttons: {
                ok: {
                    text: '提交反馈',
                    btnClass: 'btn-blue',
                    action: function () {
                        let musicErrType = [];
                        $('input[name="musicErrType"]:checked').each(function (index, element) {
                            musicErrType.push($(this).val());
                        });
                        if ($('#err-other:checked').val()) {
                            let err_input = $('#err-other-input').val().trim();
                            if (err_input.length == 0) {
                                alert('错误描述不能为空');
                                $('#err-other-input').show().focus();
                                return false;
                            }
                            if (err_input.length > 15) {
                                alert('错误描述不能超过15个字');
                                $('#err-other-input').show().focus();
                                return false;
                            }
                            musicErrType.push(err_input);
                        }
                        if (musicErrType.length <= 0) {
                            alert('请至少选择一种错误类型');
                            return false;
                        }
                        let btn = $(this);
                        console.log(btn)
                        btn.prop('disabled', true);
                        $.ajax({
                            type: "POST",
                            url: "/api/feedback",
                            dataType: "json",
                            data: {
                                "musicErrType": musicErrType,
                                "musicId": id,
                            },
                            success: function (result) {
                                if (result.code === 1) {
                                    jAlert(result.msg, 'green', true, {icon: 'fa fa-check-circle', title: '提交成功'});
                                    return true;
                                } else {
                                    jAlert(result.msg);
                                    return false;
                                }
                            },
                            error: function (e) {
                                jAlert('网络请求失败，请稍后重试！');
                            },
                            complete: function () {
                                btn.prop('disabled', false);
                                return true;
                            }
                        });
                    }
                }
            }
        });
    }
</script>

    
    
